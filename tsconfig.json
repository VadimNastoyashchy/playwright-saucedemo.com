{
    "compilerOptions": {
      "target": "es2021",
      "lib": [
        "dom",
        "es2021"
      ],
      "module": "commonjs",
      "types": [
        "node",
      ],
      "resolveJsonModule": true,
      "allowJs": true,
      "checkJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "strict": true
    },
    "exclude": [
      "./node_modules"
    ]
  }